        <owl:disjointWith rdf:resource="https://ontohub.org/meta/lola/ontology#LogicalLanguageAspect"/>
        <owl:disjointWith rdf:resource="https://ontohub.org/meta/lola/ontology#StructuringLanguageAspect"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">the (unique) language aspect of an OMS language that enables the expression of comments and annotations</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#AnswerSubstitution -->

