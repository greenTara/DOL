    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#hasMember">
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#OMSLibrary"/>
        <owl:inverseOf rdf:resource="https://ontohub.org/meta/lola/ontology#memberOf"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#hasOMS -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#hasOMS">
        <rdfs:comment>The subject is an OMS library, in which the object is a participating heterogeneous OMS.</rdfs:comment>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <rdfs:subPropertyOf rdf:resource="https://ontohub.org/meta/lola/ontology#hasMember"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#hasPart -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#hasPart">
        <rdfs:label>has part</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">relationship between two things were the second is a component of the first</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#hasSubset -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#hasSubset">
        <rdfs:label>has subset</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">relationship between two sets where every member of the second set belongs to the first set</skos:definition>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#Set"/>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#Set"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#implicitlyExtendsTo -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#implicitlyExtendsTo">
        <rdfs:label>implicitly extends to</rdfs:label>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#ImpliedExtensionMapping"/>
        <rdfs:subPropertyOf rdf:resource="https://ontohub.org/meta/lola/ontology#definitionallyExtendsTo"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#impliedExtensionOf -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#impliedExtensionOf">
        <rdfs:subPropertyOf rdf:resource="https://ontohub.org/meta/lola/ontology#definitionallyExtends"/>
        <strictlyStrongerThan rdf:resource="https://ontohub.org/meta/lola/ontology#definitionallyExtends"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#interpretationIn -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#interpretationIn"/>
    


    <!-- https://ontohub.org/meta/lola/ontology#isAdjointOf -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isAdjointOf">
        <rdfs:label>is adjoint of</rdfs:label>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#Mapping"/>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#Mapping"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isApproximantOf -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isApproximantOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>is approximant of</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">approximation (in the sense of a logically implied theory, possibly aftersuitable translation) of an OMS in a smaller signature or OMS language</skos:definition>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isConservativeExtensionOf -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isConservativeExtensionOf">
        <rdfs:label>is conservative extension of</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="https://ontohub.org/meta/lola/ontology#extends"/>
        <owl:inverseOf rdf:resource="https://ontohub.org/meta/lola/ontology#isModuleOf"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#isMappedBy"/>
            <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#conservativelyExtendsTo"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isDefinitionalExtensionOf -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isDefinitionalExtensionOf">
        <rdfs:label>is definitional extension of</rdfs:label>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> $O_2$ being a definitional extension of $O_1$ implies a bijective correspondence between the classes of models of $O_2$ and $O_1$. </skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> An extension $O_2$ of an OMS $O_1$ is a definitional extension, if each model of $O_1$ can be uniquely expanded to a model of $O_2$. </skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> Each definitional extension is also a monomorphic extension but not vice versa. </skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">extension whose newly introduced non-logical symbols are interpreted in a unique way</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://ontohub.org/meta/lola/ontology#isConservativeExtensionOf"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#isMappedBy"/>
            <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#definitionallyExtendsTo"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isElementOf -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isElementOf">
        <rdfs:label>is element of OMS</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">any resource in an OMS (e.g. a non-logical symbol, a sentence, a correspondence, the OMS itself, ...) or a named set of such resources.</skos:definition>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <rdfs:subPropertyOf rdf:resource="https://ontohub.org/meta/lola/ontology#hasPart"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isFormalizedBy -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isFormalizedBy">
        <rdfs:label>is formalized by</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">institution that formalizes a logic</skos:definition>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#Institution"/>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#Logic"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isImpliedExtensionOf -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isImpliedExtensionOf">
        <rdfs:label>is implied extension of</rdfs:label>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> A conservative extension $O_2$ of an OMS $O_1$ is an implied extension, if and only if the signature of $O_2$ is the signature of $O_1$. $O_2$ is an implied extension of $O_1$ if and only if the model class of $O_2$ is the model class of $O_1$. </skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> Each implied extension is also a definitional extension but not vice versa. </skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">model-theoretic conservative extension that does not introduce new non-logical symbols} </skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://ontohub.org/meta/lola/ontology#isDefinitionalExtensionOf"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#isMappedBy"/>
            <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#implicitlyExtendsTo"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isLanguageAspectOf -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isLanguageAspectOf">
        <rdfs:label>is language aspect of</rdfs:label>
        <owl:inverseOf rdf:resource="https://ontohub.org/meta/lola/ontology#hasLanguageAspect"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isLogicallyInteroperableWith -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isLogicallyInteroperableWith">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:label>is logically interoperable with</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">TBD</skos:definition>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#StructuredOMS"/>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#StructuredOMS"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isMappableTo -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isMappableTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>is mappable to</rdfs:label>
        <skos:editorialNote rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">reification of this property, and its subproperties, is illustrated by property chain</skos:editorialNote>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#isMappedBy"/>
            <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#mapsTo"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isMappedBy -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isMappedBy">
        <rdfs:label>is mapped by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isModelOfOMS -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isModelOfOMS">
        <rdfs:label>is a model of the OMS</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">relationship between a model and an OMS where the model is a model of the signature of the OMS and further satisfies all sentences in the OMS.</skos:definition>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#Model"/>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isModelOfSignature -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isModelOfSignature">
        <rdfs:label>is a model of the signature</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">relationship between a model and a signature (vocabulary) such that the model provides a semantic interpretation of all non-logical symbols in the signature</skos:definition>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#Model"/>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#Vocabulary"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isModuleOf -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isModuleOf">
        <rdfs:label>is module of</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">relationship between two OMSs where the first conservatively extends to the second OMS</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://ontohub.org/meta/lola/ontology#isSubOMSOf"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isOMSDocumentationOf -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isOMSDocumentationOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label>is OMS documentation of</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">relationship between an OMS documentation and an OMS where TBD</skos:definition>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#OMSDocumentation"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isOMSUnionOf -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isOMSUnionOf">
        <rdfs:label>is union of OMSs</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">aggregation of several OMSs to a new OMS, without any renaming</skos:definition>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#SetOfOMSs"/>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#StructuredOMS"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isSubLanguageOf -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isSubLanguageOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>is sublanguage of</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">syntactically specified subset of a given language, consisting of a subset of its terminal and nonterminal symbols and grammar rules</skos:definition>
        <rdfs:comment>The subject is a sublanguage of the object language.  Supported logics are shared along the inverse of this property.</rdfs:comment>
        <todo:question>Do we need both isSubLanguageOf and profileOf?  Intuitively I do think so, and I think it&apos;s reasonable to distinguish them via their properties wrt. logic/serialization propagation.</todo:question>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#Language"/>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#Language"/>
        <rdfs:subPropertyOf rdf:resource="https://ontohub.org/meta/lola/ontology#mappableToLanguage"/>
        <rdfs:subPropertyOf rdf:resource="https://ontohub.org/meta/lola/ontology#translatableTo"/>
    </owl:ObjectProperty>
    <owl:Axiom>
        <terms:date rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#date">2012-04-17</terms:date>
        <owl:annotatedTarget>Do we need both isSubLanguageOf and profileOf?  Intuitively I do think so, and I think it&apos;s reasonable to distinguish them via their properties wrt. logic/serialization propagation.</owl:annotatedTarget>
        <terms:creator>Christoph Lange</terms:creator>
        <owl:annotatedProperty rdf:resource="http://example.org/todo#question"/>
        <owl:annotatedSource rdf:resource="https://ontohub.org/meta/lola/ontology#isSubLanguageOf"/>
    </owl:Axiom>
    


    <!-- https://ontohub.org/meta/lola/ontology#isSubLogicOf -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isSubLogicOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>is sublogic of</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">a logic that is a syntactic restriction of another logic, inheriting its semantics</skos:definition>
        <rdfs:comment>The subject is a sublogic of the object.</rdfs:comment>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#Logic"/>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#Logic"/>
        <rdfs:subPropertyOf rdf:resource="https://ontohub.org/meta/lola/ontology#embeddableInto"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description>
                <owl:inverseOf rdf:resource="https://ontohub.org/meta/lola/ontology#exactLogicalExpressivity"/>
            </rdf:Description>
            <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#isSubLanguageOf"/>
            <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#exactLogicalExpressivity"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#isSubOMSOf -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#isSubOMSOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>is subOMS of</rdfs:label>
        <rdfs:label>is combination of OMSs</rdfs:label>
        <skos:example rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">Consider an ontology involving a concept \texttt{Person}, and another one involving \texttt{Human being}, and an alignment that relates these to concepts. In the combination of the ontologies along the alignment, there is only one concept, representing both \texttt{Person} and \texttt{Human being}.</skos:example>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">OMS whose sets of non-logical symbols and sentences are subsets of those present in a given larger OMS</skos:definition>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">aggregation of all the OMS in a OMS graph, where non-logical symbols are shared according to the OMS mappings in the OMS graph} </skos:definition>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#SetOfOMSs"/>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#StructuredOMS"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#language -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#language">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>the language of an OMS</rdfs:comment>
        <rdfs:comment>The reasoners in Protégé don&apos;t like us to say the following.  Together with the SubPropertyChain specified for supportsSerialization it would go beyond OWL DL anyway.
SubPropertyChain: serialization o serializes</rdfs:comment>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#OMSLanguage"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#logic -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#logic">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>the logic in which an OMS is formalized</rdfs:comment>
        <todo:question>Inside a &apos;logic section&apos; in the abstract syntax, there are also views.  Therefore, do we need another, analogous RDF property for &apos;the logic of a view&apos;?</todo:question>
        <rdfs:range rdf:resource="https://ontohub.org/meta/lola/ontology#Logic"/>
        <rdfs:domain rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
    </owl:ObjectProperty>
    <owl:Axiom>
        <terms:date rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#date">2011-12-04</terms:date>
        <owl:annotatedTarget>Inside a &apos;logic section&apos; in the abstract syntax, there are also views.  Therefore, do we need another, analogous RDF property for &apos;the logic of a view&apos;?</owl:annotatedTarget>
        <terms:creator>Christoph Lange</terms:creator>
        <owl:annotatedProperty rdf:resource="http://example.org/todo#question"/>
        <owl:annotatedSource rdf:resource="https://ontohub.org/meta/lola/ontology#logic"/>
    </owl:Axiom>
    


    <!-- https://ontohub.org/meta/lola/ontology#mappableFrom -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#mappableFrom">
        <rdfs:comment>There is a mapping from the object to the subject.</rdfs:comment>
        <owl:inverseOf rdf:resource="https://ontohub.org/meta/lola/ontology#mappableTo"/>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#mappableFromLanguage -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#mappableFromLanguage">
        <rdfs:comment>There is a mapping from the object language to the subject language.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#mappableFromLogic -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#mappableFromLogic">
        <rdfs:comment>There is a mapping from the object logic to the subject logic.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#mappableTo -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#mappableTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>There is a mapping from the subject to the object.</rdfs:comment>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#sourceOfMapping"/>
            <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#mapsTo"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://ontohub.org/meta/lola/ontology#mappableToLanguage -->

    <owl:ObjectProperty rdf:about="https://ontohub.org/meta/lola/ontology#mappableToLanguage">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>There is a mapping from the subject language to the object language.</rdfs:comment>
        <owl:inverseOf rdf:resource="https://ontohub.org/meta/lola/ontology#mappableFromLanguage"/>
