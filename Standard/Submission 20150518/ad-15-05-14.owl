    <!-- https://ontohub.org/meta/lola/ontology#Approximation -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Approximation">
        <rdfs:label>approximation</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LogicalOMSMapping"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">logical OMS mapping to a logically implied theory (possibly after suitable translation) in a smaller signature or OMS language</skos:definition>
        <skos:editorialNote rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">this definition is derived from the defintion of approximant in the spec</skos:editorialNote>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#AxiomSentence -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#AxiomSentence">
        <rdfs:label>axiom sentence</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Sentence"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">sentence postulated to be valid (i.e.\ true in every model)</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#BasicOMS -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#BasicOMS">
        <rdfs:label>basic OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#FlattenableOMS"/>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#StructuredOMS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#hasPart"/>
                <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#SetOfSentences"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#hasPart"/>
                <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#Vocabulary"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#hasPart"/>
                <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#OMSDocumentation"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> The sentences must use only those non-logical symbols that are present in the signature. </skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">signature equipped with a set of sentences and annotations, which may be used as a building block for a larger OMS</skos:definition>
        <rdfs:comment>a basic OMS (in a single language, logic, and serialization)</rdfs:comment>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#ConsequenceTheoreticConservativeExtensionMappin -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#ConsequenceTheoreticConservativeExtensionMappin">
        <rdfs:label>consequence-theoretic conservative extension mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#ConservativeExtensionMapping"/>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">An extension $O_2$ of an OMS $O_1$ is a consequence-theoretic conservative extension, if all properties formulated in the signature of $O_1$ hold for $O_1$ whenever they hold for $O_2$.</skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">extension that does not add new theorems (in terms of the unextended signature)</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#ConservativeExtensionMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#ConservativeExtensionMapping">
        <rdfs:label>conservative extension mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#ExtensionMapping"/>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">If used without qualification, the consequence-theoretic version is meant.</skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">consequence-theoretic or model-theoretic conservative extension</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#DefaultMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#DefaultMapping">
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LogicMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#DefinitionalExtensionMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#DefinitionalExtensionMapping">
        <rdfs:label>definitional extension mapping</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#ModelTheoreticConservativeExtensionMapping"/>
                    <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#WeakDefinitionalExtensionMapping"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#MonomorphicExtensionMapping"/>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">$O_2$ being a definitional extension of $O_1$ implies a bijective correspondence between the classes of models of $O_2$ and $O_1$.</skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">An extension $O_2$ of an OMS $O_1$ is a definitional extension, if each model of $O_1$ can be uniquely expanded to a model of $O_2$.</skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">Each definitional extension is also a monomorphic extension but not vice versa.</skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">extension whose newly introduced non-logical symbols are interpreted in a unique way</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#ElusiveOMS -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#ElusiveOMS">
        <rdfs:label>elusive OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <owl:disjointWith rdf:resource="https://ontohub.org/meta/lola/ontology#FlattenableOMS"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">OMS that is not flattenable</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Embedding -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Embedding">
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#ExactMapping"/>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LogicMapping"/>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#ModelBijectiveMapping"/>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Translation"/>
        <reifies rdf:resource="https://ontohub.org/meta/lola/ontology#embeddableInto"/>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Equivalence -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Equivalence">
        <rdfs:label>equivalence</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LogicalOMSMapping"/>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> Two OMSs are equivalent if they have a common definitional extension. The OMSs may be written in different OMS languages. </skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">logical OMS mapping ensuring that two OMSs share the same definable concepts} </skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#ExactMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#ExactMapping">
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#WeaklyExactMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <reifies rdf:resource="https://ontohub.org/meta/lola/ontology#exactlyMappableTo"/>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Expression -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Expression">
        <rdfs:label>expression</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#usesLanguage"/>
                <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#Language"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">TBD see Class &apos;language&apos;</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#ExtensionMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#ExtensionMapping">
        <rdfs:label>extension mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LogicalOMSMapping"/>
        <skos:editorialNote rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">This definition differs from that in the OntoIOp glossary by focussing on the mapping rather than the codomain of the mapping.</skos:editorialNote>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">mapping between two OMSs where the sets of non-logical symbols and sentences of the second OMS are supersets of those present in the first OMS</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#FaithfulMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#FaithfulMapping">
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LogicMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <reifies rdf:resource="https://ontohub.org/meta/lola/ontology#faithfullyMappableTo"/>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#FlattenableOMS -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#FlattenableOMS">
        <rdfs:label>flattenable OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">More precisely, an OMS is flattenable if and only if it is either a basic OMS or it is an extension, union, translation, module extraction, approximation, filtering, or reference of named OMS involving only flattenable OMS.</skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">OMS that can be seen, by purely syntactical means, to be logically equivalent to a basic OMS</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#FocusedOMS -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#FocusedOMS">
        <rdfs:label>focused OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">TBD</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Graph -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Graph">
        <rdfs:label>graph</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">TBD</skos:definition>
        <skos:altLabel rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">diagram</skos:altLabel>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#HeterogeneousEnvironment -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#HeterogeneousEnvironment">
        <rdfs:label>heterogeneous environment</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#OMSLanguageGraph"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#OMSLogicGraph"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#SupportsRelation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">environment for the expression of homogeneous and heterogeneous OMS, comprising a logic graph, an OMS language graph and a supports relation</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#HeterogeneousOMS -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#HeterogeneousOMS">
        <rdfs:label>heterogeneous OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <owl:disjointWith rdf:resource="https://ontohub.org/meta/lola/ontology#HomogeneousOMS"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">OMS whose parts are supported by different logics</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#HomogeneousOMS -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#HomogeneousOMS">
        <rdfs:label>homogeneous OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">OMS whose parts are supported by one logic</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#ImpliedExtensionMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#ImpliedExtensionMapping">
        <rdfs:label>implied extension mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#DefinitionalExtensionMapping"/>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">A conservative extension $O_2$ of an OMS $O_1$ is an implied extension, if and only if the signature of $O_2$ is the signature of $O_1$. $O_2$ is an implied extension of $O_1$ if and only if the model class of $O_2$ is the model class of $O_1$.</skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">Each implied extension is also a definitional extension but not vice versa.</skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">model-theoretic conservative extension that does not introduce new non-logical symbols</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Institution -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Institution">
        <rdfs:label>institution</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#formalizes"/>
                <owl:someValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#Logic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">metaframework mathematically formalising the notion of a logic</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Interpretation -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Interpretation">
        <owl:equivalentClass rdf:resource="https://ontohub.org/meta/lola/ontology#View"/>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#OMSMapping"/>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Language -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Language">
        <rdfs:label>language</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#SetOfExpressions"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">collection of expressions</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#LanguageAspect -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#LanguageAspect">
        <rdfs:label>language aspect</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">set of language constructs of a given language, not necessarily forming a sublanguage</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#LanguageMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#LanguageMapping">
        <rdfs:label>language mapping</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#mapsFrom"/>
                        <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#Language"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#mapsTo"/>
                        <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#Language"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Mapping"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">mapping between languages</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#LinkedData -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#LinkedData">
        <rdfs:label>linked data</rdfs:label>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> RDF, serialized as RDF/XML \cite{W3C:REC-rdf-syntax-grammar-20040210}, is the most common format for publishing linked data. However, its usage is not mandatory. </skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> The linked data principles (adapted from \cite{BernersLee:LinkedData2006} and its paraphrase at \cite{Wikipedia:LinkedData2011}) are the following: \begin{enumerate} \item Use IRIs as names for things. \item Use HTTP IRIs so that these things can be referred to and looked up (``dereferenced&apos;&apos;) by people and user agents.\footnote{I.e., the IRI is treated as a URL (uniform resource locator).} \item Provide useful machine-processable (plus optionally human-readable) information about the thing when its IRI is dereferenced, using standard formats. \item Include links to other, related IRIs in the exposed data to improve discovery of other related information on the Web. \end{enumerate} </skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> Using HTTP content negotiation \cite{rfc2616} it is possible to serve representations in different formats from the same URL. </skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">structured data that is published on the Web in a machine-processable way, according to principles specified in \cite{BernersLee:LinkedData2006,BizerHeath09}}</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Logic -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Logic">
        <rdfs:label>logic</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#isFormalizedBy"/>
                <owl:someValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#Institution"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:example rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> $\mathcal{SROIQ}(D)$ is the logic underlying OWL 2 DL. </skos:example>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> Most OMS languages have an underlying logic. </skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> See annex~\ref{a:rdf-logic-vocab} for the organization of the relation between OMS languages and their logics and serializations. </skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">specification of valid reasoning that comprises signatures, sentences, models, and a satisfaction relation between models and sentences</skos:definition>
        <rdfs:comment>a logic that defines the semantics of an language</rdfs:comment>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#LogicApproximation -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#LogicApproximation">
        <rdfs:label>logic approximation</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LogicMapping"/>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> A unique maximal approximation need not exist. </skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">mapping of a source logic onto a (usually less expressive) target logic that tries to approximate the OMSs expressed in the source logic with means of the expressivity of the target logic</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#LogicMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#LogicMapping">
        <rdfs:label>logic mapping</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#mapsFrom"/>
                        <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#Logic"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#mapsTo"/>
                        <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#Logic"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Mapping"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#mapsFrom"/>
                        <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#Logic"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#mapsTo"/>
                        <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#Logic"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://ontohub.org/meta/lola/ontology#OMSLanguageTranslation"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">mapping between logics</skos:definition>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <rdfs:comment>a mapping (translation or reduction) between two logics</rdfs:comment>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#LogicReduction -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#LogicReduction">
        <rdfs:label>logic reduction</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LogicMapping"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">mapping of a source logic onto a (usually less expressive) target logic (mapping signatures, sentences and models) that simply forgets those parts of the logical structure not fitting the target logic</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#LogicTranslation -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#LogicTranslation">
        <rdfs:label>logic translation</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LogicMapping"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">mapping of a source logic into a target logic (mapping signatures, sentences and models) that keeps or encodes the logical content of OMSs</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#LogicalLanguageAspect -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#LogicalLanguageAspect">
        <rdfs:label>logical language aspect</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LanguageAspect"/>
        <owl:disjointWith rdf:resource="https://ontohub.org/meta/lola/ontology#StructuringLanguageAspect"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">the (unique) language aspect of an OMS language that enables the expression of non-logical symbols and sentences in a logical language</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#LogicalOMSMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#LogicalOMSMapping">
        <rdfs:label>logical OMS mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#OMSMapping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#SetOfCorrespondences"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> Logical OMS mappings are given as sets of correspondences, which are required to be \emph{signature morphisms}. </skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">OMS mapping that has a formal, logic-based semantics</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#LogicalTheory -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#LogicalTheory">
        <rdfs:label>logical theory</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#hasPart"/>
                <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#SetOfSentences"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#hasPart"/>
                <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#Vocabulary"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">signature equipped with a set of sentences over the signature</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#MDEModel -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#MDEModel">
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <rdfs:comment>a model (in the sense of model-driven engineering (MDE))</rdfs:comment>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Mapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Mapping">
        <rdfs:label>mapping</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#mapsFrom"/>
                        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#mapsTo"/>
                        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">TBD</skos:definition>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">in some cases is a morphism, as in Category Theory.</skos:note>
        <rdfs:comment>a generic mapping (translation or reduction)</rdfs:comment>
        <reifies rdf:resource="https://ontohub.org/meta/lola/ontology#mappableTo"/>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Model -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Model">
        <rdfs:label>model</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#isModelOfSignature"/>
                <owl:someValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#Vocabulary"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> semantic interpretation of a signature</skos:definition>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">A model of an OMS is a model of the signature of the OMS that moreover satisfies all the axioms of the OMS.</skos:note>
        <skos:scopeNote rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">This term is not to be confused with model (here called MDEModel) in the sense of modeling (i.e., the ``M&apos;&apos; in OMS).</skos:scopeNote>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#ModelBijectiveMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#ModelBijectiveMapping">
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#ModelExpansiveMapping"/>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#WeaklyExactMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <reifies rdf:resource="https://ontohub.org/meta/lola/ontology#modelBijectivelyMappableTo"/>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#ModelExpansiveMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#ModelExpansiveMapping">
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#FaithfulMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <reifies rdf:resource="https://ontohub.org/meta/lola/ontology#modelExpansivelyMappableTo"/>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#ModelTheoreticConservativeExtensionMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#ModelTheoreticConservativeExtensionMapping">
        <rdfs:label>model-theoretic conservative extension mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#ConservativeExtensionMapping"/>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">An extension $O_2$ of an OMS $O_1$ is a model-theoretic conservative extension, if all properties formulated in the signature of $O_1$ hold for $O_1$ whenever they hold for $O_2$.</skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">Any model-theoretic conservative extension is also a consequence-theoretic one.</skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">extension mapping that does not lead to a restriction of class of models of an OMS</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#ModuleExtraction -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#ModuleExtraction">
        <rdfs:label>module extraction</rdfs:label>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">The goal of module extraction is ``decomposing an OMS into smaller, more manageable modules with appropriate dependencies&apos;&apos; \cite{DBLP:series/lncs/5445}</skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">activity of obtaining from an OMS concrete modules to be used for a particular purpose (e.g. to contain a particular sub-signature of the original OMS)</skos:definition>
        <SpecificationMetadata:informativeReference>Cited and slightly adapted from \cite{SuarezFigueroaEtAl:OntologyGlossary2008}</SpecificationMetadata:informativeReference>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#MonomorphicExtensionMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#MonomorphicExtensionMapping">
        <rdfs:label>monomorphic extension mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#ModelTheoreticConservativeExtensionMapping"/>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">An extension $O_2$ of an OMS $O_1$ is a monomorphic extension, if each model of $O_1$ can be expanded to a model of $O_2$ that is unique up to isomorphism.</skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">Each monomorphic extension is also a model-theoretic conservative extension but not vice versa.</skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">extension whose newly introduced non-logical symbols are interpreted in a way unique up to isomorphism</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#NonLogicalSymbol -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#NonLogicalSymbol">
        <rdfs:label>non-logical symbol</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Symbol"/>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Term"/>
        <skos:example rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> Non-logical symbols in Common Logic \nisref{ISO/IEC 24707:2007} comprise \begin{itemize} \item names (denoting objects from the domain of discourse), \item sequence markers (denoting sequences of objects). \end{itemize} This is opposed to logical symbols in Common Logic, e.g.\ logical connectives and quantifiers. </skos:example>
        <skos:example rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> Non-logical symbols in OWL \nisref{W3C/TR REC-owl2-syntax:2009} (there called ``entities&apos;&apos;) comprise \begin{itemize} \item individuals (denoting objects from the domain of discourse), \item classes (denoting sets of objects; also called concepts), and \item properties (denoting binary relations over objects; also called roles). \end{itemize} This is opposed to logical symbols in OWL, e.g.\ those for intersection and union of classes. </skos:example>
        <skos:altLabel rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">OMS symbol</skos:altLabel>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">OMS symbol atomic expression or syntactic constituent of an OMS that requires an interpretation through a model</skos:definition>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">The notion of ``atomic sentence&apos;&apos; used in logic is different, it usually may involve several non-logical symbols. </skos:note>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#OMS -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#OMS">
        <rdfs:label>OMS (ontology, specification or model)</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#HeterogeneousOMS"/>
                    <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#HomogeneousOMS"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#ElusiveOMS"/>
                    <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#FlattenableOMS"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#usesLanguage"/>
                <owl:someValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#OMSLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">An OMS can be written in different OMS language serializations.</skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">collection of expressions (like non-logical symbols, sentences and structuring elements) in a given OMS language (or several such languages)</skos:definition>
        <rdfs:comment>a basic OMS or an OMS library</rdfs:comment>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#OMSDocumentation -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#OMSDocumentation">
        <rdfs:label>OMS documentation</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">set of all annotations to an OMS, plus any other documents and explanatory comments generated during the entire OMS building process</skos:definition>
        <SpecificationMetadata:informativeReference>  
    Adapted from \cite{SuarezFigueroaEtAl:OntologyGlossary2008}
  </SpecificationMetadata:informativeReference>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#OMSGraph -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#OMSGraph">
        <rdfs:label>OMS graph</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Graph"/>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> An OMS graph is a diagram of OMS in the sense of category theory, but different from a diagram in the sense of model-driven architecture.</skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> graph with OMSs as nodes and OMS mappings as edges, showing how the OMS are interlinked </skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#OMSLanguage -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#OMSLanguage">
        <rdfs:label>OMS language</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Language"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#supportsLogic"/>
                        <owl:someValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#Logic"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#supportsSerialization"/>
                        <owl:someValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#Serialization"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#supportsLogic"/>
                        <owl:allValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#Logic"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#supportsSerialization"/>
                        <owl:allValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#Serialization"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#supportsLogic"/>
                <owl:someValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#Logic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> An OMS language is used for the formal specification of OMSs.</skos:note>
        <skos:example rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> OMS languages include OWL, Common Logic, F-logic, UML class diagrams, RDFS, and OBO. </skos:example>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">language equipped with a formal, declarative, logic-based semantics, plus non-logical annotations</skos:definition>
        <rdfs:comment>a language for OMS (ontologies, models or specifications)</rdfs:comment>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#OMSLanguageGraph -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#OMSLanguageGraph">
        <rdfs:label>OMS language graph</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Graph"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">TBD</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#OMSLanguageTranslation -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#OMSLanguageTranslation">
        <rdfs:label>OMS language translation</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LanguageMapping"/>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Mapping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#mapsFrom"/>
                <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#OMSLanguage"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#mapsTo"/>
                <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#OMSLanguage"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#mapsFrom"/>
                        <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#OMSLanguage"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#mapsTo"/>
                        <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#OMSLanguage"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> An OMS language translation shall satisfy the property that the result of a translation is a well-formed text in the target language. </skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">mapping from constructs in the source OMS language to their equivalents in the target OMS language</skos:definition>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <rdfs:comment>a mapping (translation or reduction) between two OMS languages</rdfs:comment>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#OMSLibrary -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#OMSLibrary">
        <rdfs:comment>an OMS library</rdfs:comment>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#OMSLogicGraph -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#OMSLogicGraph">
        <rdfs:label>OMS logic graph</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Graph"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">TBD</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#OMSMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#OMSMapping">
        <rdfs:label>OMS mapping</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#mapsFrom"/>
                        <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#mapsTo"/>
                        <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Mapping"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">relationship between two OMSs, typically given as a set of correspondences</skos:definition>
        <rdfs:comment>a mapping between two heterogeneous OMSs in an OMS library</rdfs:comment>
        <reifies rdf:resource="https://ontohub.org/meta/lola/ontology#mappableTo"/>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#OMSNetwork -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#OMSNetwork">
        <rdfs:label>OMS network</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">collection of named OMSs, possibly written in different OMS languages, linked by named OMS mappings and named OMS graphs</skos:definition>
        <skos:altLabel rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">hyperontology</skos:altLabel>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Ontology -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Ontology">
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <rdfs:comment>an ontology</rdfs:comment>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#PlainMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#PlainMapping">
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LogicMapping"/>
        <owl:disjointWith rdf:resource="https://ontohub.org/meta/lola/ontology#SimpleTheoroidalMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <rdfs:comment>Protégé 4.2 doesn&apos;t like &apos;Class: OMSMapping DisjointUnionOf: PlainMapping, SimpleTheoroidalMapping&apos;, so we do it this way:</rdfs:comment>
        <reifies rdf:resource="https://ontohub.org/meta/lola/ontology#plainlyMappableTo"/>
    </owl:Class>
    <owl:Axiom>
        <rdfs:comment>Protégé 4.2 doesn&apos;t like &apos;Class: OMSMapping DisjointUnionOf: PlainMapping, SimpleTheoroidalMapping&apos;, so we do it this way:</rdfs:comment>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2002/07/owl#disjointWith"/>
        <owl:annotatedSource rdf:resource="https://ontohub.org/meta/lola/ontology#PlainMapping"/>
        <owl:annotatedTarget rdf:resource="https://ontohub.org/meta/lola/ontology#SimpleTheoroidalMapping"/>
    </owl:Axiom>
    


    <!-- https://ontohub.org/meta/lola/ontology#Profile -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Profile">
        <rdfs:label>profile</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#OMSLanguage"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#supportsLogic"/>
                <owl:onClass rdf:resource="https://ontohub.org/meta/lola/ontology#Logic"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> Profiles can have different logics, even with completely different semantics, e.g.\ OWL 2 DL versus OWL 2 Full.</skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> The logic needs to support the language. </skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">(syntactic) sublanguage of an OMS language interpreting according to a particular logic that targets specific applications or reasoning methods} </skos:definition>
        <skos:example rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">Profiles of OWL 2 include OWL 2 EL, OWL 2 QL, OWL 2 RL, OWL 2 DL, and OWL 2 Full.</skos:example>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">Profiles typically correspond to sublogics. </skos:note>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Query -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Query">
        <rdfs:label>query</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Sentence"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">sentence containing query variables that can be instantiated by a substitution</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#QueryLanguage -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#QueryLanguage">
        <rdfs:label>query language</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#OMSLanguage"/>
        <skos:example rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> SPARQL, Prolog </skos:example>
        <skos:scopeNote rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> There are also general purpose OMS languages, which can express both OMS and quries. </skos:scopeNote>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">OMS language specifically dedicated to queries</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#QueryVariable -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#QueryVariable">
        <rdfs:label>query variable</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Symbol"/>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">From an abstract point of view, query variables are just symbols; they are used in a way that they will be substituted using a substitution. Many OMS languages have special notations for (query) variables.</skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">If there are no variables in an OMS language, constants can be used as query variables.</skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">Usually, query variables are the free variables of a sentence, whereas there can be other (bound) variables.</skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">symbol that will be used in a query and a substitution</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Reduction -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Reduction">
        <rdfs:label>reduction</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LogicalOMSMapping"/>
        <owl:disjointWith rdf:resource="https://ontohub.org/meta/lola/ontology#Translation"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">logical OMS mapping reducing an OMS to a smaller signature</skos:definition>
        <rdfs:comment>a generic reduction</rdfs:comment>
        <reifies rdf:resource="https://ontohub.org/meta/lola/ontology#reducibleTo"/>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Refinement -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Refinement">
        <rdfs:label>refinement</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LogicalOMSMapping"/>
        <rdfs:comment>refinement</rdfs:comment>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Renaming -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Renaming">
        <rdfs:label>renaming</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LogicalOMSMapping"/>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> A renaming results in a logical OMS mapping between the original and the renamed OMS. </skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">assignment of new names to some non-logical symbols of an OMS</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Resource -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Resource">
        <rdfs:label>resource</rdfs:label>
        <skos:example rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">Familiar examples include an electronic document, an image, a source of information with a consistent purpose (e.g., ``today&apos;s weather report for Los Angeles&apos;&apos;), a service (e.g., an HTTP-to-SMS gateway), and a collection of other resources. A resource is not necessarily accessible via the Internet; e.g., human beings, corporations, and bound books in a library can also be resources. Likewise, abstract concepts can be resources, such as the operators and operands of a mathematical equation, the types of a relationship (e.g., ``parent&apos;&apos; or ``employee&apos;&apos;), or numeric values (e.g., zero, one, and infinity). \nisref{IETF/RFC 3986:2005, Section 1.1}</skos:example>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">\nisref{IETF/RFC 3986:2005, Section 1.1} deliberately defines a resource as ``in a general sense \textelp{} whatever might be identified by \textins*{an IRI}&apos;&apos;. The original source refers to URIs, but DOL uses the compatible IRI standard \nisref{IETF/RFC 3987:2005} for identification.</skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">something that can be globally identified} </skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#SatisfactionRelation -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#SatisfactionRelation">
        <rdfs:label>satisfaction relation</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">relation between models and sentences indicating which sentences hold true in the model</skos:definition>
        <rdfs:seeAlso rdf:resource="http://www.omg.org/spec/OntoIOp/OntoIOpTerminology/satisfies"/>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Sentence -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Sentence">
        <rdfs:label>sentence</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Term"/>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">A sentence can conform to one or more signatures (namely those signatures containing all non-logical symbols used in the sentence).</skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">In a model, on the one hand, a sentence is always true or false. In an OMS, on the other hand, a sentence can have several logical statuses: it can be an axiom, if postulated to be true; a theorem, if proven from other axioms and theorems; a conjecture, if expecting to be proven from other axioms and theorems; or have another of many possible statuses.</skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">It is quite common that sentences are required to be closed (i.e.\ have no free variables). However, this depends on the OMS language at hand.</skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">\CLnote[type=fyi]{From Common Logic, I changed ``unit of logical text&apos;&apos; to ``term&apos;&apos;.}}</skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">term that is either true or false in a given model, i.e.\ which is assigned a truth value in this model.</skos:definition>
        <rdfs:comment>a sentence in an OMS</rdfs:comment>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Serialization -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Serialization">
        <rdfs:label>OMS serialization</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontohub.org/meta/lola/ontology#serializes"/>
                <owl:someValuesFrom rdf:resource="https://ontohub.org/meta/lola/ontology#OMSLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:example rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string"> Common Logic uses the term ``dialect&apos;&apos;; the following are standard Common Logic dialects: Common Logic Interchange Format (CLIF), Conceptual Graph Interchange Format (GCIF), eXtended Common Logic Markup Language (XCL).</skos:example>
        <skos:example rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">OWL uses the term ``serialization&apos;&apos;; the following are standard OWL serializations: OWL functional-style syntax, OWL/XML, OWL Manchester syntax, plus any standard serialization of RDF (e.g.  RDF/XML, Turtle, \dots). However, RDF/XML is the only one tools are required to implement. </skos:example>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">Serializations serve as standard formats for exchanging OMSs between tools.</skos:note>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">specific syntactic encoding of a given OMS language</skos:definition>
        <rdfs:comment>a serialization of an language</rdfs:comment>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#Set -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Set">
        <rdfs:label>set</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">set whose members have a certain type, as specified by the type parameter.</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#SetOfAnnotationExpressions -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#SetOfAnnotationExpressions">
        <rdfs:label>set of annotation expressions</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#SetOfExpressions"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">set whose members are annotation expressions.</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#SetOfCorrespondences -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#SetOfCorrespondences">
        <rdfs:label>set of correspondences</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#SetOfMappings"/>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#SetOfExpressions -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#SetOfExpressions">
        <rdfs:label>set of expressions</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Set"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">set whose members are expressions</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#SetOfMappings -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#SetOfMappings">
        <rdfs:label>set of mappings</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Set"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">set whose members are mappings.</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#SetOfNonlogicalSymbols -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#SetOfNonlogicalSymbols">
        <rdfs:label>set of non-logical symbols</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#SetOfExpressions"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">set whose members are non-logical symbols</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#SetOfOMSs -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#SetOfOMSs">
        <rdfs:label>set of OMSs</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Set"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">set whose members are OMSs.</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#SetOfSentences -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#SetOfSentences">
        <rdfs:label>set of sentences</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#SetOfExpressions"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">set whose members are sentences</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#SimpleTheoroidalMapping -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#SimpleTheoroidalMapping">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://ontohub.org/meta/lola/ontology#LogicMapping"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="https://ontohub.org/meta/lola/ontology#PlainMapping"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LogicMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <rdfs:comment>orthogonal to WeaklyExactMapping and FaithfulMapping</rdfs:comment>
        <reifies rdf:resource="https://ontohub.org/meta/lola/ontology#simplyTheoroidallyMappableTo"/>
    </owl:Class>
    <owl:Axiom>
        <rdfs:comment>Protégé 4.2 doesn&apos;t like &apos;Class: OMSMapping DisjointUnionOf: PlainMapping, SimpleTheoroidalMapping&apos;, so we do it this way:</rdfs:comment>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2002/07/owl#disjointWith"/>
        <owl:annotatedSource rdf:resource="https://ontohub.org/meta/lola/ontology#PlainMapping"/>
        <owl:annotatedTarget rdf:resource="https://ontohub.org/meta/lola/ontology#SimpleTheoroidalMapping"/>
    </owl:Axiom>
    


    <!-- https://ontohub.org/meta/lola/ontology#Specification -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#Specification">
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <rdfs:comment>a formal specification</rdfs:comment>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#StandoffMarkup -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#StandoffMarkup">
        <rdfs:label>standoff markup</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Language"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">way of providing annotations to subjects in external resources, without embedding them into the original resource (here: OMS)</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#StructuredOMS -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#StructuredOMS">
        <rdfs:label>structured OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#OMS"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">OMS that results from other OMSs by import, union, combination, renaming or other structuring operations</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#StructuringLanguageAspect -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#StructuringLanguageAspect">
        <rdfs:label>structuring language aspect</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#LanguageAspect"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#string">the (unique) language aspect of an OMS language that covers structured OMSs as well as the relations of basic OMSs and structured OMSs to each other, including, but not limited to imports, OMS mappings, conservative extensions, and the handling of prefixes for CURIEs</skos:definition>
    </owl:Class>
    


    <!-- https://ontohub.org/meta/lola/ontology#SubLogic -->

    <owl:Class rdf:about="https://ontohub.org/meta/lola/ontology#SubLogic">
        <rdfs:subClassOf rdf:resource="https://ontohub.org/meta/lola/ontology#Embedding"/>
        <rdfs:comment>&apos;syntactic&apos; embedding</rdfs:comment>
