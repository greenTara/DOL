#baseline for FTF
LASTREV=7f584ed0
BRANCH=$(git branch | grep "*" |cut -b 3-)
OUT=OMG_OntoIOp_current-diff-$BRANCH.tex
DIFF=OMG_OntoIOp_current-diff$LASTREV.tex
latexdiff-git -r $LASTREV --force --exclude-textcmd="section,subsection" OMG_OntoIOp_current.tex 
echo "working on problematic environments"
python listing_highlight.py $DIFF $OUT
echo "generated $OUT"
exit 0

